{
  "name": "msoffice2pdf",
  "version": "1.0.4",
  "description": "Convert Microsoft Office documents (ppt, pptx, doc, docx, xls, xlsx) to PDF.",
  "main": "dist/build.common.js",
  "module": "dist/build.esm.js",
  "types": "types/index.d.ts",
  "files": [
    "dist/*.js",
    "docs",
    "types",
    "CHANGELOG.md",
    "hero.svg",
    "LANGUAGE-LIST.md",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "watch": "rollup -c --watch --bundleConfigAsCjs",
    "prewatch": "rimraf dist types",
    "build": "rollup -c --bundleConfigAsCjs",
    "prebuild": "rimraf dist types",
    "test": "jest"
  },
  "_moduleAliases": {
    "~": "dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/takuya-motoshima/msoffice2pdf.git"
  },
  "keywords": [
    "office",
    "ppt",
    "pptx",
    "doc",
    "docx",
    "xls",
    "xlsx",
    "pdf",
    "convert"
  ],
  "author": "Takuya Motoshima <developer.takuyamotoshima@gmail.com> (https://twitter.com/TakuyaMotoshima)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/takuya-motoshima/msoffice2pdf/issues",
    "email": "developer.takuyamotoshima@gmail.com"
  },
  "homepage": "https://github.com/takuya-motoshima/msoffice2pdf#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-json": "^5.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.1.0",
    "@types/glob": "^8.0.0",
    "@types/node": "^18.11.9",
    "@types/tmp": "^0.2.3",
    "jest": "^29.3.1",
    "module-alias": "^2.2.2",
    "rimraf": "^3.0.2",
    "rollup": "^3.2.5",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-loader": "^8.1.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.2.9",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "glob": "^8.0.3",
    "tmp": "^0.2.1"
  }
}
